    </HelpFiles>
    <Dictionary>
      <Include>CommonDataTypes.tod</Include>
      <Include>Modules.tod</Include>
      <Include>DT_cfg_CIB.tod</Include>
      <!-- zakladni datove typy konfiguracnich dialogu CIB modulu -->
      <Include>DT_ini_CIB.tod</Include>
      <!-- zakladni datove typy inicializacnich tabulek CIB modulu -->
      <Include>DT_BIN_CIB.tod</Include>
      <!-- zakladni datove typy binarnich i/o CIB modulu -->
      <Include>DT_AI_CIB.tod</Include>
      <!-- zakladni datove typy analogovych vstupu CIB modulu -->
      <Include>DT_AO_CIB.tod</Include>
      <!-- zakladni datove typy analogovych vystupu CIB modulu -->
      <Include>DT_oth_CIB.tod</Include>
      <!-- zakladni datove typy ostatnich i/o CIB modulu -->
      <DataTypes>
        <!-- dilci struktury konfiguracniho dialogu modulu -->
        <!-- struktura konfiguracniho dialogu modulu  -->
        <DataType>
          <Name>TDevices_3319</Name>
          <SubItems>
            <SubItem>
              <Name>StateDevice</Name>
              <!-- status AI -->
              <Type>BOOL</Type>
            </SubItem>
            <SubItem>
              <Name>ThermAnalogInput1Device</Name>
              <!-- AI1 -->
              <Type>TCIB_ThermAnalogInputDeviceCfg</Type>
            </SubItem>
            <SubItem>
              <Name>ThermAnalogInput2Device</Name>
              <!-- AI2 -->
              <Type>TCIB_ThermAnalogInputDeviceCfg</Type>
            </SubItem>
          </SubItems>
        </DataType>

        <!-- struktura inicializacni tabulky cib modulu -->
        <DataType>
          <Name>TCIB_3319_init</Name>
          <SubItems>
            <SubItem>
              <Name>Header</Name>
              <!-- hlavicka -->
              <Type>_TCIBHeader_</Type>
            </SubItem>
            <SubItem>
              <Name>Dev</Name>
              <!-- zarizeni -->
              <Type>_TDev[3]_</Type>
            </SubItem>
            <SubItem>
              <Name>therm1offset</Name>
              <!-- offset teplomeru 1 -->
              <Type>INT</Type>
            </SubItem>
            <SubItem>
              <Name>therm2offset</Name>
              <!-- offset teplomeru 2 -->
              <Type>INT</Type>
            </SubItem>
          </SubItems>
        </DataType>

        <!-- i/o struktury modulu -->
        <DataType>
          <Name>TCIB_C_IT_0200R_IN</Name>
          <SubItems>
            <SubItem>
              <Name>STAT</Name>
              <Type>TCIB_CIT0200_STAT</Type>
              <BitOffs>0</BitOffs>
            </SubItem>
            <SubItem>
              <Name>iTHERM</Name>
              <Type>REAL</Type>
              <BitOffs>8</BitOffs>
            </SubItem>
            <SubItem>
              <Name>eTHERM</Name>
              <Type>REAL</Type>
              <BitOffs>40</BitOffs>
            </SubItem>
          </SubItems>
        </DataType>

      </DataTypes>

      <Objects>
        <!-- konfiguracni dialog modulu (objekty #x8000 ...) -->
        <Object index="#x8000" name="Devices" type="TDevices_3319" access="rw">
          <DisplayName>
            <label lang="cz">Zařízení</label>
            <label lang="en">Devices</label>
          </DisplayName>
          <SubObjects>
            <SubObject name="StateDevice" access="rw">
              <!-- status, STAT -->
              <DisplayName>
                <label lang="cz">Status</label>
                <label lang="en">State</label>
              </DisplayName>
              <DefaultValue>1</DefaultValue>
              <!-- defaultne zarizeni povolit -->
            </SubObject>
            <SubObject name="ThermAnalogInput1Device" access="rw">
              <!-- analogovy vstup, AI1, interni teplomer -->
              <DisplayName>
                <label lang="cz">Interní teploměr</label>
                <label lang="en">Internal thermometer</label>
              </DisplayName>
              <SubObjects>
                <SubObject name="Enabled" access="rw">
                  <DefaultValue>1</DefaultValue>
                  <!-- defaultne zarizeni povolit -->
                </SubObject>
                <SubObject name="ThermOffset" access="rw">
                  <DisplayName>
                    <label lang="cz">Offset teploty [°C]</label>
                    <label lang="en">Offset temperature [°C]</label>
                  </DisplayName>
                  <DefaultValue>0</DefaultValue>
                  <MinValue>-50.0</MinValue>
                  <!-- -50.0°C -->
                  <MaxValue>50.0</MaxValue>
                  <!-- +50.0°C -->
                </SubObject>
              </SubObjects>
            </SubObject>
            <SubObject name="ThermAnalogInput2Device" access="rw">
              <!-- analogovy vstup, AI2, externi teplomer -->
              <DisplayName>
                <label lang="cz">Externí teploměr</label>
                <label lang="en">External thermometer</label>
              </DisplayName>
              <SubObjects>
                <SubObject name="Enabled" access="rw">
                  <DefaultValue>0</DefaultValue>
                  <!-- defaultne zarizeni zakazat -->
                </SubObject>
                <SubObject name="ThermOffset" access="rw">
                  <DisplayName>
                    <label lang="cz">Offset teploty [°C]</label>
                    <label lang="en">Offset temperature [°C]</label>
                  </DisplayName>
                  <DefaultValue>0</DefaultValue>
                  <MinValue>-50.0</MinValue>
                  <!-- -50.0°C -->
                  <MaxValue>50.0</MaxValue>
                  <!-- +50.0°C -->
                </SubObject>
              </SubObjects>
            </SubObject>
          </SubObjects>
        </Object>

        <!-- inicializacni tabulka modulu (objekty #x9000 ...) -->
        <Object index="#x9000" name="r{RACK}_p{POSITION}_C_IT_0200R_init" type="TCIB_3319_init" access="rw">
          <SubObjects>
            <SubObject name="Header" access="rw">
              <!-- hlavicka ini. tabulky -->
              <SubObjects>
                <SubObject name="code" access="ro">
                  <!-- kod modulu -->
                  <DefaultValue>3319</DefaultValue>
                </SubObject>
                <SubObject name="mode" access="ro">
                  <DefaultValue>0</DefaultValue>
                </SubObject>
                <SubObject name="fadr" access="rw">
                  <!-- HW adresa modulu (dlouha adresa) -->
                  <MacroValue>${FADR}</MacroValue>
                </SubObject>
                <SubObject name="ladr" access="rw">
                  <!-- logicka adresa modulu (kratka adresa) -->
                  <MacroValue>{POSITION}</MacroValue>
                </SubObject>
                <SubObject name="ndev" access="ro">
                  <!-- pocet zarizeni modulu -->
                  <DefaultValue>3</DefaultValue>
                </SubObject>
              </SubObjects>
            </SubObject>
            <SubObject name="Dev" access="rw">
              <!-- zarizeni modulu -->
              <SubObjects>
                <SubObject name="Dev[0]" access="rw">
                  <!-- zarizeni 1, vstupni, STAT -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <DefaultData>80</DefaultData>
                      <!-- defaultne povolit -->
                      <ValueDependency>
                        <When condition="@Devices.StateDevice == 0">
                          <Data>00</Data>
                        </When>
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>8</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
                <SubObject name="Dev[1]" access="rw">
                  <!-- zarizeni 2, vstupni, AI1, interni teplmer -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <DefaultData>80</DefaultData>
                      <!-- defaultne povolit -->
                      <ValueDependency>
                        <When condition="@Devices.ThermAnalogInput1Device.Enabled == 0">
                          <Data>00</Data>
                        </When>
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>16</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
                <SubObject name="Dev[2]" access="rw">
                  <!-- zarizeni 3, vstupni, AI2, externi teplomer -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <DefaultData>00</DefaultData>
                      <!-- defaultne zakazat -->
                      <ValueDependency>
                        <When condition="@Devices.ThermAnalogInput2Device.Enabled == 1">
                          <Data>80</Data>
                        </When>
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>16</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
              </SubObjects>
            </SubObject>
            <SubObject name="therm1offset" access="rw">
              <!-- offset teploty ai1 -->
              <DefaultValue>0</DefaultValue>
              <ValueDependency>
                <Formula>@Devices.ThermAnalogInput1Device.ThermOffset * 100</Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="therm2offset" access="rw">
              <!-- offset teploty ai2 -->
              <DefaultValue>0</DefaultValue>
              <ValueDependency>
                <Formula>@Devices.ThermAnalogInput2Device.ThermOffset * 100</Formula>
              </ValueDependency>
            </SubObject>
          </SubObjects>
        </Object>

        <!-- i/o data modulu -->
        <!-- vstupni data modulu (objekty #x6000 ...) -->
        <!-- stavovy byte stat-->
        <!-- analogovy vstup ai1 -->
        <!-- analogovy vstup ai2 -->
        <!-- vystupni data modulu (objekty #x7000 ...) -->
        <Object index="#x6000" name="r{RACK}_p{POSITION}_IN" type="TCIB_C_IT_0200R_IN">
          <SubObjects>
            <SubObject name="STAT">
              <DisplayName>
                <label lang="cz">STAT</label>
                <label lang="en">STAT</label>
              </DisplayName>
              <Disabled>@Devices.StateDevice == 0</Disabled>
              <!-- zakazani objektu -->
            </SubObject>
            <SubObject name="iTHERM">
              <DisplayName>
                <label lang="cz">iTHERM</label>
                <label lang="en">iTHERM</label>
              </DisplayName>
              <Transformation>
                <Conversion>93</Conversion>
                <!-- teplotu konvertovat znamenkove v setinach, SW100TF -->
              </Transformation>
              <Disabled>(@Devices.ThermAnalogInput1Device.Enabled == 0)</Disabled>
              <!-- zakazani objektu -->
            </SubObject>
            <SubObject name="eTHERM">
              <DisplayName>
                <label lang="cz">eTHERM</label>
                <label lang="en">eTHERM</label>
              </DisplayName>
              <Transformation>
                <Conversion>93</Conversion>
                <!-- teplotu konvertovat znamenkove v setinach, SW100TF -->
              </Transformation>
              <Disabled>(@Devices.ThermAnalogInput2Device.Enabled == 0)</Disabled>
              <!-- zakazani objektu -->
            </SubObject>
          </SubObjects>
        </Object>
        
        
<!-- =============================================================== -->
<!-- Module declaration for CP-2xxx --> 
<!-- =============================================================== -->

        <!-- inicializacni tabulka driveru                           -->
        <!-- Indexy inicializacnich objektu:                         -->
        <!-- $01 - Nazev zarizeni {PRODUCT_NAME_LEN}{PRODUCT_NAME}   -->
        <!-- $02 - Alias zarizeni {DEVICE_ALIAS_LEN}{DEVICE_ALIAS}   -->
        <!-- $03 - Konverze vstupu {CONV_IN_LEN},{$CONV_IN_ARRAY}    -->
        <!-- $04 - Konverze vystupu {CONV_OUT_LEN},{$CONV_OUT_ARRAY} -->
        
        <Object index="#x9001" name="r{RACK}_p{POSITION}_DriverInit" type="BYTE" access="rw">
          <Comment>
            <label lang="cz">Inicializacni tabulka driveru</label>
            <label lang="en">Initialization table of driver</label>
          </Comment>
          <Excluded>&quot;{SYSTEM}&quot; != &quot;FOXTROT2&quot;</Excluded>
          <MacroValue>$01,{PRODUCT_NAME_LEN},'{PRODUCT_NAME}',\n$02,{DEVICE_ALIAS_LEN},'{DEVICE_ALIAS}',\n$03,{CONV_IN_LEN},{$CONV_IN_ARRAY},\n$00</MacroValue>
        </Object>

        <Object index="#xA000" name="r{RACK}_p{POSITION}_Module" type="TModulE2" access="rw"> <!-- if="_PLCTYPE_ &gt; 10" -->
          <Excluded>&quot;{SYSTEM}&quot; != &quot;FOXTROT2&quot;</Excluded> 
          <SubObjects> 
            <SubObject name="version" access="ro">   
              <DefaultValue>2</DefaultValue>            
            </SubObject>  
            <SubObject name="hwCode" access="ro">   
              <MacroValue>{HWCODE}</MacroValue>            
            </SubObject>
            <SubObject name="busId" access="ro">   
              <MacroValue>${$RACK}</MacroValue>
            </SubObject>
            <SubObject name="address" access="ro">   
              <MacroValue>{POSITION}</MacroValue>
            </SubObject>
            <SubObject name="port" access="ro">   
              <DefaultValue>9</DefaultValue>            
            </SubObject>
            <SubObject name="lenInputs" access="ro">   
              <MacroValue>__sizeof(r{RACK}_p{POSITION}_IN)</MacroValue>             
            </SubObject>
            <SubObject name="lenOutputs" access="ro">   
              <MacroValue>0</MacroValue>             
            </SubObject>
            <SubObject name="offsetInputs" access="ro">
              <MacroValue>__offset(r{RACK}_p{POSITION}_IN)</MacroValue>
            </SubObject>
            <SubObject name="offsetOutputs" access="ro">
              <MacroValue>0</MacroValue>
            </SubObject>
            <SubObject name="initTable" access="ro">
              <MacroValue>__indx(r{RACK}_p{POSITION}_C_IT_0200R_init)</MacroValue>
            </SubObject>
            <SubObject name="driverInitTable" access="ro">
              <MacroValue>__indx(r{RACK}_p{POSITION}_DriverInit)</MacroValue>
            </SubObject>
          </SubObjects>
        </Object>

      </Objects>

    </Dictionary>
  </Device>
</TecoDescriptor>
