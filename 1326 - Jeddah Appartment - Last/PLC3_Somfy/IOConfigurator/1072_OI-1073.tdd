              </ValueDependency>
            </SubObject>
            <SubObject name="AREP" access="rw">
              <DefaultValue>15</DefaultValue>
              <ValueDependency>
                <ValueOf>AutorepeatDelay</ValueOf>
              </ValueDependency>
            </SubObject>
            <SubObject name="MARK" access="rw">
              <DefaultData>80</DefaultData>
              <ValueDependency>
                <When condition="@UseEndDelimiter == 0">
                  <Data>00</Data>
                </When>
              </ValueDependency>
            </SubObject>
          </SubObjects>
        </Object>  
        
